{
    "name": "vonec/gitsync",
    "type": "library",
    "description": "Laravel Package to deploy code from git to multiple autoscaling EC2 instances on AWS",
    "keywords": [
        "vonec",
        "gitsync",
        "aws codedeploy",
        "autoscaled instances"
    ],
    "homepage": "https://github.com/vonec/gitsync",
    "license": "MIT",
    "authors": [
        {
            "name": "Vonec Technologies",
            "email": "vamsi@vonectech.com",
            "homepage": "http://www.vonectech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0",
        "aws/aws-sdk-php": "^3.29",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "vonec\\gitsync\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "vonec\\gitsync\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
